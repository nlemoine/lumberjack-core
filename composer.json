{
    "name": "rareloop/lumberjack-core",
    "description": "A powerful MVC framework for the modern WordPress developer. Write better, more expressive and easier to maintain code",
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "blast/facades": "^1.0",
        "http-interop/response-sender": "^1.0",
        "laminas/laminas-diactoros": "^2.4",
        "mindplay/middleman": "^3.0.3",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.0.0",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.1",
        "rareloop/psr7-server-request-extension": "^2.1.0",
        "rareloop/router": "^4.4.0",
        "spatie/macroable": "^1.0",
        "statamic/stringy": "^3.1",
        "symfony/console": "^5.2",
        "symfony/error-handler": "^5.1",
        "symfony/http-foundation": "^5.2",
        "symfony/http-kernel": "^5.2",
        "tightenco/collect": "^8.0",
        "timber/timber": "^1.8"
    },
    "require-dev": {
        "antecedent/patchwork": "^2.1.8",
        "brain/monkey": "^2.6",
        "dms/phpunit-arraysubset-asserts": "^0.3.0",
        "mikey179/vfsstream": "^1.6.8",
        "mockery/mockery": "^1.3.0",
        "php-coveralls/php-coveralls": "^2.0",
        "php-mock/php-mock": "^2.0",
        "phpunit/phpunit": "^8.5 || ^9.0",
        "symplify/easy-coding-standard": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Rareloop\\Lumberjack\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rareloop\\Lumberjack\\Test\\": "tests"
        }
    },
    "scripts": {
        "lint": [
            "vendor/bin/ecs check"
        ],
        "lint:fix": [
            "vendor/bin/ecs check --fix"
        ],
        "test": [
            "vendor/bin/phpunit --testdox"
        ]
    }
}
